<h1>Listing proposalls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proposalls.each do |proposall| %>
      <tr>
        <td><%= link_to 'Show', proposall %></td>
        <td><%= link_to 'Edit', edit_proposall_path(proposall) %></td>
        <td><%= link_to 'Destroy', proposall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proposall', new_proposall_path %>
